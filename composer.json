{
  "name": "kejwmen/phpunit-listeners",
  "description": "",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "kejwmen\\PhpUnitListeners\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kejwmen\\PhpUnitListeners\\Test\\": "test"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/console": "^3.3|^4.0",
    "lstrojny/functional-php": "^1.6",
    "beberlei/assert": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpstan/phpstan": "^0.10",
    "doctrine/coding-standard": "^5.0",
    "phpstan/phpstan-beberlei-assert": "^0.10.0"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse -l max src test",
    "cs": "vendor/bin/phpcs --standard=Doctrine src test",
    "cs-fix": "vendor/bin/phpcbf --standard=Doctrine src test"
  }
}
